{
  "opts": {
    "destination": "docs",
    "package": "",
    "readme": "readme.md"
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  },
  "source": {
    "includePattern": ".+\\.(js|ts)$"
  },
  "plugins": ["node_modules/better-docs/typescript"],
  "tags": {
    "allowUnknownTags": true
  }
}